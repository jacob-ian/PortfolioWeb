rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if false 
    }

    match /qualifications/{qualificationId} {
      allow read: if true
      allow write: if false
    }

    match /qualifications/{qualificationId}/subjects/{subjectId} {
      allow read: if true
      allow write: if false
    }
  }
}